{
  "private": true,
  "main": "DECAD",
  "version": "0.3.0",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "npm run compile && electron-builder",
    "dist:dir": "npm run dist --dir -c.compression=store -c.mac.identity=null"
  },
  "electronWebpack": {
    "title": true,
    "staticSourceDirectory": "website",
    "main": {
      "sourceDirectory": "src/Main",
      "webpackConfig": "webpack.additions.main.js"
    },
    "renderer": {
      "sourceDirectory": "src/Renderer",
      "template": "website/index.html",
      "webpackConfig": "webpack.additions.renderer.js"
    }
  },
  "dependencies": {
    "bulma": "^0.9.0",
    "file-loader": "^3.0.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12",
    "node-sass": "^4.12.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rechart": "^0.0.1",
    "sass-loader": "^7.1.0",
    "source-map-support": "^0.5.16",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "bufferutil": "^4.0",
    "devtron": "^1.4",
    "electron": "^8.0.0",
    "electron-builder": "^22.3.2",
    "electron-devtools-installer": "^3.1.0",
    "electron-webpack": "^2.6",
    "electron-window-state": "^5.0",
    "fable-compiler": "^2.4.13",
    "fable-loader": "^2.1",
    "node-sass": "^4.13.1",
    "remotedev": "^0.2",
    "sass-loader": "^8.0.2",
    "utf-8-validate": "^5.0",
    "webpack": "^4.41.6"
  }
}
